@model IEnumerable<HR_System.Models.t_objetivos>

@{
    ViewBag.Title = "Direct Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h6 class="font-weight-bold text-center grey-text text-uppercase small mb-2 mt-3" style="font-size:1.5vw">MODULE 1</h6>
<h3 class="font-weight-bold text-center  pb-2" style="font-size:3.5vw">Direct Reports</h3>

<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.estatus)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.t_empleados.nombre)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.axo)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.estatus)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.t_empleados.nombre)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.axo)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
